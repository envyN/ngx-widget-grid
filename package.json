{
  "name": "ngx-wg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod --base-href=/ngx-widget-grid/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "build-lib": "ng build ngx-widget-grid --prod",
    "build-lib:w": "ng build ngx-widget-grid --watch",
    "npm-pack": "cd dist/ngx-widget-grid && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish-dry": "cd dist/ngx-widget-grid && npm publish --dry-run",
    "publish-lib": "cd dist/ngx-widget-grid && npm publish",
    "publish:dry": "npm run package && npm run publish-dry",
    "publish": "npm run package && npm run publish-lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@clr/angular": "^4.0.1",
    "@clr/icons": "^4.0.1",
    "@clr/ui": "^4.0.1",
    "core-js": "^3.6.4",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "@clr/core": "^4.0.1",
    "@webcomponents/webcomponentsjs": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
