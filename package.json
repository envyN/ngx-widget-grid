{
  "name": "ngx-wg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build-lib",
    "start": "ng serve -o",
    "build": "ng build --prod --base-href=/ngx-widget-grid/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build ngx-widget-grid",
    "build-lib:w": "ng build ngx-widget-grid --watch",
    "npm-pack": "cd dist/ngx-widget-grid && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish-dry": "cd dist/ngx-widget-grid && npm publish --dry-run",
    "publish-lib": "cd dist/ngx-widget-grid && npm publish",
    "publish:dry": "npm run package && npm run publish-dry",
    "publish": "npm run package && npm run publish-lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@clr/angular": "^2.3.7",
    "@clr/icons": "^2.3.7",
    "@clr/ui": "^2.3.7",
    "@webcomponents/custom-elements": "^1.0.0",
    "core-js": "^3.6.4",
    "rxjs": "~6.5.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.17.15",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.6.2",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
