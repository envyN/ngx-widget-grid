{
  "name": "ngx-wg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --configuration production --base-href=/ngx-widget-grid/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:readme": "cp README.md projects/ngx-widget-grid",
    "postinstall": "ngcc",
    "prebuild-lib": "npm run copy:readme",
    "build-lib": "ng build ngx-widget-grid --configuration production",
    "build-lib:w": "ng build ngx-widget-grid --watch",
    "npm-pack": "cd dist/ngx-widget-grid && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish-dry": "cd dist/ngx-widget-grid && npm publish --dry-run",
    "publish-lib": "cd dist/ngx-widget-grid && npm publish",
    "publish:dry": "npm run package && npm run publish-dry",
    "publish": "npm run package && npm run publish-lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@clr/angular": "~15.4.0",
    "@clr/icons": "~13.0.2",
    "@clr/ui": "~15.4.0",
    "core-js": "^3.6.4",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.13.0",
    "@clr/core": "^4.0.1",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "@cds/core": "~6.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~18.15.13",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  }
}
